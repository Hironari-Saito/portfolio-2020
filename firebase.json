{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "headers": [{
    "source" : "**",
    "headers" : [{
      "key" : "Cache-Control",
      "value" : "max-age=31536000"
    }]
  }]
}
